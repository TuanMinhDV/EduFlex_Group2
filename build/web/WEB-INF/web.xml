<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>CourseListController</servlet-name>
        <servlet-class>controller.common.DungBT.CourseListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChapterListController</servlet-name>
        <servlet-class>controller.common.DungBT.ChapterListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuizzListController</servlet-name>
        <servlet-class>controller.common.DungBT.QuizzListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuestionListController</servlet-name>
        <servlet-class>controller.common.DungBT.QuestionListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LessonListController</servlet-name>
        <servlet-class>controller.common.DungBT.LessonListController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CourseListController</servlet-name>
        <url-pattern>/listCourse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChapterListController</servlet-name>
        <url-pattern>/listChapter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuizzListController</servlet-name>
        <url-pattern>/listQuizz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestionListController</servlet-name>
        <url-pattern>/listQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LessonListController</servlet-name>
        <url-pattern>/listLession</url-pattern>
    </servlet-mapping>
</web-app>
